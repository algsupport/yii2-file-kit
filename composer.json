{
    "name": "algsupport/yii2-file-kit",
    "description": "Yii2 file upload and storage kit",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Albert Gukasian",
            "email": "admin@algsupport.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "yiisoft/yii2-jui": "*",
        "league/flysystem": "*",
        "npm-asset/blueimp-file-upload": "*",
        "rmrevin/yii2-fontawesome": "*"
    },
    "autoload": {
        "psr-4": {
            "algsupport\\filekit\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
