{
    "name": "algsupport/yii2-file-kit",
    "description": "Yii2 file upload and storage kit",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Albert Gukasian",
            "email": "admin@algsupport.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "algsupport/yii2-jui": "*",
        "league/flysystem": "*",
		"algsupport/yii2-bootstrap-icons": "*"
    },
    "autoload": {
        "psr-4": {
            "algsupport\\filekit\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
